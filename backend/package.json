{
  "name": "zuvomo-backend",
  "version": "1.0.0",
  "description": "Zuvomo Backend API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cp .env.development .env && npm run dev",
    "start:prod": "cp .env.production .env && npm run start",
    "validate:env": "node scripts/validateEnv.js",
    "env:dev": "cp .env.development .env && echo 'Switched to development environment'",
    "env:prod": "cp .env.production .env && echo 'Switched to production environment'",
    "check:env": "npm run validate:env"
  },
  "keywords": [
    "zuvomo",
    "backend",
    "api",
    "authentication"
  ],
  "author": "Zuvomo Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
